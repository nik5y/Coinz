<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="300dp"
    android:layout_alignParentStart="true"
    android:layout_gravity="center"

    android:background="#ecd499"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <TextView
        android:id="@+id/maps_dialog_rates_quid_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="36dp"
        android:layout_toStartOf="@+id/maps_dialog_rates_quid_value"
        android:text="Currency"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/maps_dialog_rates_quid_value"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/maps_dialog_rates_dolr_title"
        app:layout_constraintTop_toBottomOf="@+id/maps_dialog_rates_dolr_title"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/maps_dialog_rates_dolr_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/maps_dialog_rates_peny_title"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:layout_toStartOf="@+id/maps_dialog_rates_dolr_value"
        android:text="Currency"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/maps_dialog_rates_dolr_value"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/maps_dialog_rates_peny_title"
        app:layout_constraintTop_toBottomOf="@+id/maps_dialog_rates_peny_title" />

    <TextView
        android:id="@+id/maps_dialog_rates_peny_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:layout_toStartOf="@+id/maps_dialog_rates_peny_value"
        android:text="Currency"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/maps_dialog_rates_peny_value"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/maps_dialog_rates_shil_title"
        app:layout_constraintTop_toBottomOf="@+id/maps_dialog_rates_shil_title" />

    <TextView
        android:id="@+id/maps_dialog_rates_shil_title"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="100dp"
        android:layout_toStartOf="@+id/maps_dialog_rates_shil_value"
        android:text="Currency"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/maps_dialog_rates_shil_value"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/maps_dialog_rates_quid_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:text="Value"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/maps_dialog_rates_dolr_value"
        app:layout_constraintTop_toBottomOf="@+id/maps_dialog_rates_dolr_value"
        app:layout_constraintVertical_bias="0.437" />

    <TextView
        android:id="@+id/maps_dialog_rates_dolr_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="32dp"
        android:text="Value"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/maps_dialog_rates_peny_value"
        app:layout_constraintTop_toBottomOf="@+id/maps_dialog_rates_peny_value" />

    <TextView
        android:id="@+id/maps_dialog_rates_peny_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="32dp"
        android:text="Value"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/maps_dialog_rates_shil_value"
        app:layout_constraintTop_toBottomOf="@+id/maps_dialog_rates_shil_value" />

    <TextView
        android:id="@+id/maps_dialog_rates_shil_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="16dp"
        android:text="Value"
        android:textColor="@android:color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>

